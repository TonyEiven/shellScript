####funds#####
#早起打卡获取e卡列表
location ^~ /api/v3/ecard-dew/ {
	proxy_pass http://pay.tairanmall.com/api/v3/ecard-dew/;
}


#调E卡红包的接口
location ^~ /api/v3/gateway/ {
	proxy_pass   http://epay.tairanmall.com/api/v3/gateway/;
}





####pay#####
####调node-gateway程旭的网关
location ^~ /api/v3/gateway/ {
	proxy_pass  http://funds-api-gateway/gateway/;
}

location ^~ /api/v3/ecard-dew/ {
	proxy_pass http://ecard-dew/ecard-dew/;
}